<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oa.me.modules.news.mapper.ArticleTagMapper">

    <select id="queryArticleTags" resultType="Integer">
        SELECT
           tag_id
        FROM article_tag
       where article_id=#{id}
    </select>

</mapper>